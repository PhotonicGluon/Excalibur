{
    "files.exclude": {
        "**/__pycache__": true,
        ".pytest_cache": true,
        ".python-version": true,
        ".ruff_cache": true,
        ".venv": true
    },
    "cSpell.words": [
        "asgi",
        "decryptor",
        "encryptor",
        "exef",
        "fastapi",
        "keysize",
        "maxfail",
        "priv",
        "secho"
    ],
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "black-formatter.args": [
        "--line-length=120"
    ]
}
