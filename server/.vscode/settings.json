{
    "files.exclude": {
        "**/__pycache__": true,
        ".python-version": true,
    },
    "cSpell.words": [
        "exef",
        "fastapi",
        "keysize",
        "maxfail",
        "priv",
        "secho"
    ],
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "black-formatter.args": [
        "--line-length=120"
    ]
}
