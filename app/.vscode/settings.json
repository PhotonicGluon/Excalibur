{
    "files.exclude": {
        "node_modules": true
    },
    "cSpell.words": [
        "asar",
        "capawesome",
        "comlink",
        "exef",
        "filelike",
        "fullpath",
        "keysize",
        "listdir",
        "maskito",
        "mimetypes",
        "popover",
        "powmod",
        "priv",
        "randbits",
        "updaters"
    ],
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
}
